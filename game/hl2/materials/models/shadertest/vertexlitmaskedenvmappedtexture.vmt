"VertexLitGeneric"
{
	"$basetexture" "models/shadertest/VertexLitTexture"
	"$envmap" "shadertest/shadertest_env"
	"$envmapmask" "shadertest/shadertest_envmask"
	"$multipass" 1

	// Color green + blue is sinusoidal
	"Proxies"
	{
		"Sine"
		{
			"resultVar" "$envmaptint[0]"
			"sineperiod" 1.00
			"sinemin" 0
			"sinemax" 1
		}

		"Sine"
		{
			"resultVar" "$color[1]"
			"sineperiod" 7.00
			"sinemin" 0
			"sinemax" 10
		}

		// Alpha is sinusoidal
		"Sine"
		{
			"resultVar" "$alpha"
			"sineperiod" 10.00
			"sinemin" 0.5
			"sinemax" 0.5
		}
	}
}

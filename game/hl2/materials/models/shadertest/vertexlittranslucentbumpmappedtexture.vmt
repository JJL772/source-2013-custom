"VertexLitGeneric"
{
	"$basetexture" "models/shadertest/vertexlitalphatestedtexture"
	"$bumpmap" "shadertest/bumpy_normal"
	"$translucent" 1
	"$model" 1
	"$bumpoffset" "[0 0]"

	// Color green + blue is sinusoidal
	"Proxies"
	{
		"Sine"
		{
			"resultVar" "$color[1]"
			"sineperiod" 10.00
			"sinemin" 0
			"sinemax" 10
		}

		// Alpha is sinusoidal
		"Sine"
		{
			"resultVar" "$alpha"
			"sineperiod" 10.00
			"sinemin" 0
			"sinemax" 20
		}
		// Scroll the bumpmap
		"Sine"
		{
			"resultVar" "$bumpoffset[0]"
			"sineperiod" 40.00
			"sinemin" 2
			"sinemax" -2
		}
		"TextureTransform"
		{
			"translateVar" "$bumpoffset"
			"resultVar" "$bumptransform"
		}
	}
}

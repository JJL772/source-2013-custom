"VertexLitGeneric"
{
	"$bumpmap" "shadertest/bumpy_normal"
	"$envmap" "shadertest/cubemap"
	"$model" 1
	"$bumpoffset" "[0 0]"

	// Color green + blue is sinusoidal
	"Proxies"
	{
		"Sine"
		{
			"resultVar" "$envmaptint[0]"
			"sineperiod" 7.00
			"sinemin" 0
			"sinemax" 2
		}

		"Sine"
		{
			"resultVar" "$color[1]"
			"sineperiod" 7.00
			"sinemin" 0
			"sinemax" 2
		}

		// Alpha is sinusoidal
		"Sine"
		{
			"resultVar" "$alpha"
			"sineperiod" 10.00
			"sinemin" 0
			"sinemax" 3
		}

		// Scroll the bumpmap
		"Sine"
		{
			"resultVar" "$bumpoffset[0]"
			"sineperiod" 10.00
			"sinemin" 6
			"sinemax" -6
		}
		"Sine"
		{
			"resultVar" "$bumpoffset[1]"
			"sineperiod" 13.41
			"sinemin" -1
			"sinemax" 1
		}
		"TextureTransform"
		{
			"translateVar" "$bumpoffset"
			"resultVar" "$bumptransform"
		}
	}
}

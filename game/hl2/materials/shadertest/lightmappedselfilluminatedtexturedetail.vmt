"LightmappedGeneric"
{
	"$basetexture" "shadertest/LightmappedSelfIlluminatedTexture"
	"$selfillum" 1
	"$detail" "shadertest/shadertestdetail"

	"$temp" 0
	"$one" 1

	"Proxies"
	{
		// Texture rotates sinusoidally
		"Sine"
		{
			"resultVar" "$temp"
			"sineperiod" 30.00
			"sinemin" 0
			"sinemax" 360.0
		}

		"TextureTransform"
		{
			"rotateVar" "$temp"
			"resultVar" "$basetexturetransform"
		}

		// Color green is sinusoidal
		"Sine"
		{
			"resultVar" "$color[1]"
			"sineperiod" 10.00
			"sinemin" 0
			"sinemax" 1
		}

		// Detail scale is sinusoidal
		"Sine"
		{
			"resultVar" "$detailscale"
			"sineperiod" 10.00
			"sinemin" 2
			"sinemax" 20
		}

		// Self Illum tint gets less red as we move
		"PlayerSpeed"
		{
			"resultVar" "$temp"
			"scale" 0.01
		}

		"Subtract"
		{
			"srcVar1"	"$one"
			"srcVar2"	"$temp"
			"resultVar"	"$selfillumtint[0]" 
		}
	}
}

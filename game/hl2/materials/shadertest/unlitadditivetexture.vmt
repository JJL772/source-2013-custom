"UnlitGeneric"
{
	"$basetexture" "shadertest/UnlitAdditiveTexture"
	"$additive" 1

	"$temp" "[0 0]"

	"Proxies"
	{
		// Texture scrolls as we move!!
		"PlayerPosition"
		{
			"scale" 0.01
			"resultVar" "$temp"
		}

		"TextureTransform"
		{
			"translateVar" "$temp"
			"resultVar" "$basetexturetransform"
		}
	}
}

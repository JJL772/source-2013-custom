{
	"$basetexture" "shadertest/LightmappedBaseAlphaMaskedEnvMappedTexture"
	"$envmap" "shadertest/shadertest_env"
	"$basealphaenvmapmask" 1
	"$multipass" 1
	"$envmapmode" 1
	"$detail" "shadertest/shadertestdetail"

	"Proxies"
